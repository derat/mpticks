{
  "name": "Mountain Project Tick Cleaner",
  "short_name": "MP Tick Cleaner",
  "version": "0.3",
  "description": "Delete repeated ticks from Mountain Project",
  "manifest_version": 2,
  "permissions": ["*://*.mountainproject.com/*", "storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
