{
  "name": "Mountain Project Tick Cleaner",
  "short_name": "MP Tick Cleaner",
  "version": "0.5",
  "description": "Delete repeated ticks from Mountain Project",
  "manifest_version": 3,
  "permissions": ["storage"],
  "host_permissions": ["*://*.mountainproject.com/*"],
  "action": { "default_popup": "popup.html" },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
